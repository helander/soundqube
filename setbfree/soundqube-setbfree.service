[Unit]
Description=setBfree
After=soundqube-jack.service
BindsTo=soundqube-jack.service

[Service]
User=lehswe
Group=lehswe
LimitRTPRIO=95
LimitMEMLOCK=infinity
ExecStart=/usr/bin/pw-jack setBfree -p /usr/share/setBfree/pgm/default.pgm
Restart=always
RestartSec=10

[Install]
WantedBy=multi-user.target


